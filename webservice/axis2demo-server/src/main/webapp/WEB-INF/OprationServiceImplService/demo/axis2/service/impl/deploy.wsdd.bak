<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from OprationServiceImplService WSDL service -->

  <service name="OprationServiceImpl" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://impl.service.axis2.demo"/>
      <parameter name="wsdlServiceElement" value="OprationServiceImplService"/>
      <parameter name="schemaQualified" value="http://impl.service.axis2.demo"/>
      <parameter name="wsdlServicePort" value="OprationServiceImpl"/>
      <parameter name="className" value="demo.axis2.service.impl.OprationServiceImplSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="OprationServiceImpl"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="search" qname="operNS:search" xmlns:operNS="http://impl.service.axis2.demo" returnQName="retNS:searchReturn" xmlns:retNS="http://impl.service.axis2.demo" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:message" xmlns:pns="http://impl.service.axis2.demo" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="noSearch" qname="operNS:noSearch" xmlns:operNS="http://impl.service.axis2.demo" returnQName="retNS:noSearchReturn" xmlns:retNS="http://impl.service.axis2.demo" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:message" xmlns:pns="http://impl.service.axis2.demo" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="search noSearch"/>

  </service>
</deployment>
